<!-- Misc templates -->
<template id="featured_products_list" name="FeaturedProducts">
    <t t-foreach="products" t-as="product">
        <t t-if="product">
            <div class="col-lg-4 col-md-6">
              <div class="single-product">
                <div class="product-img">
                  <img class="img-fluid w-100" t-att-src="product.image" alt="" />
                  <div class="p_icon">
                    <a href="#">
                      <i class="ti-eye"></i>
                    </a>
                    <a href="#">
                      <i class="ti-heart"></i>
                    </a>
                    <a href="#">
                      <i class="ti-shopping-cart"></i>
                    </a>
                  </div>
                </div>
                <div class="product-btm">
                  <a href="#" class="d-block">
                    <h4 t-esc="product.name"/>
                  </a>
                  <div class="mt-3">
                    <span t-esc="product.price"/>
                    <del t-esc="product.price"/>
                  </div>
                </div>
              </div>
            </div>
        </t>
    </t>
</template>


